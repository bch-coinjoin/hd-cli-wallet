{
  "name": "hd-cli-wallet",
  "description": "npm lib and CLI app for working with HD wallets ",
  "version": "1.0.1",
  "author": "Chris Troutner <chris.troutner@gmail.com>",
  "bin": {
    "hd-cli-wallet": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.0",
    "@psf/bch-js": "4.20.7",
    "bch-js-nft": "0.2.1",
    "bcp-js": "2.0.1",
    "bignumber.js": "9.0.0",
    "cli-table": "0.3.1",
    "cli-ux": "5.5.1",
    "collect.js": "4.16.6",
    "qrcode-terminal": "0.12.0",
    "shelljs": "0.8.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@oclif/test": "1.2.7",
    "chai": "4.2.0",
    "coveralls": "3.0.2",
    "eslint": "7.26.0",
    "eslint-config-oclif": "3.1.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-standard": "4.0.0",
    "globby": "10.0.0",
    "lodash.clonedeep": "4.5.0",
    "mocha": "7.1.2",
    "mock-fs": "4.13.0",
    "nock": "12.0.0",
    "nyc": "15.0.0",
    "prettier": "2.3.0",
    "semantic-release": "17.4.2",
    "sinon": "9.0.0",
    "standard": "16.0.3",
    "test-console": "1.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/wallets",
    "/config"
  ],
  "keywords": [
    "bch",
    "hd",
    "wallet"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "hd-cli-wallet",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "bch-coinjoin/hd-cli-wallet",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "npm run lint; TEST=unit nyc --reporter=text mocha --timeout 25000 test/commands/",
    "test:integration": "RESTAPI=fullstack.cash TEST=integration nyc --reporter=text mocha --timeout 25000 test/commands/",
    "test:integration:local": "RESTAPI=local TEST=integration nyc --reporter=text mocha  --timeout 25000",
    "test:integration:decatur": "RESTAPI=decatur TEST=integration nyc --reporter=text mocha  --timeout 25000",
    "test:temp": "TEST=integration mocha --timeout 25000 -g '#getAddressData'",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage:report": "nyc --reporter=html mocha test/commands/",
    "version": "oclif-dev readme && git add README.md",
    "semantic-release": "semantic-release",
    "lint": "standard --env mocha --fix"
  }
}
